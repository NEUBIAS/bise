{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "blazy"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/dinbror/blazy",
                    "reference": "1.8.2"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "fontawesome/fontawesome",
                "type": "drupal-library",
                "version": "5.9.0",
                "dist": {
                    "url": "https://use.fontawesome.com/releases/v5.9.0/fontawesome-free-5.9.0-web.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jackmoore/colorbox",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/jackmoore/colorbox/archive/1.x.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.7",
        "dinbror/blazy": "^1.8",
        "drupal/admin_toolbar": "2.x-dev",
        "drupal/adminimal_admin_toolbar": "1.x-dev",
        "drupal/auto_height": "^1.1",
        "drupal/backup_migrate": "^4.0",
        "drupal/betterlogin": "1.x-dev",
        "drupal/blazy": "^2.0@RC",
        "drupal/block_class": "1.x-dev",
        "drupal/block_visibility_groups": "1.x-dev",
        "drupal/bootstrap": "3.x-dev",
        "drupal/bootstrap_layouts": "^5.1",
        "drupal/bootstrap_library": "^1.11",
        "drupal/broken_link": "^3.1",
        "drupal/captcha": "^1.0",
        "drupal/chosen": "^2.5",
        "drupal/colorbox": "1.x-dev",
        "drupal/conditional_fields": "^1.0",
        "drupal/console": "^1.0.2",
        "drupal/context": "4.x-dev",
        "drupal/contribute": "^5.0@beta",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.9",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^2.0",
        "drupal/ds": "^3.1",
        "drupal/entity_browser": "^2.0",
        "drupal/eva": "^2.0",
        "drupal/feeds": "^3.0",
        "drupal/fontawesome": "2.x-dev",
        "drupal/google_analytics": "^3.0",
        "drupal/honeypot": "^1.27",
        "drupal/httpbl": "1.x-dev",
        "drupal/libraries": "^3.0",
        "drupal/linkchecker": "1.x-dev",
        "drupal/markdown": "^1.2",
        "drupal/menu_breadcrumb": "^1.11",
        "drupal/menu_token": "^1.0",
        "drupal/module_filter": "3.x-dev",
        "drupal/paragraphs": "^1.5",
        "drupal/pathauto": "^1.1",
        "drupal/popular_tags": "^1.0@alpha",
        "drupal/recaptcha": "^2.3",
        "drupal/redirect": "^1.0",
        "drupal/restui": "^1.1",
        "drupal/search_api": "^1.7",
        "drupal/search_autocomplete": "^1.1",
        "drupal/shs": "^1.0",
        "drupal/social_media": "^1.8",
        "drupal/spambot": "1.x-dev",
        "drupal/structure_sync": "^1.11",
        "drupal/synonyms": "^1.0",
        "drupal/taxonomy_import": "^1.0",
        "drupal/taxonomy_menu": "3.3",
        "drupal/token": "^1.1",
        "drupal/token_filter": "^1.0",
        "drupal/views_accordion": "1.x-dev",
        "drupal/views_bulk_edit": "^2.0",
        "drupal/views_bulk_operations": "^2.0",
        "drupal/views_parity_row": "^1.8",
        "drupal/zircon": "^1.0",
        "drush/drush": "^9.0.0",
        "harvesthq/chosen": "^1.8",
        "jackmoore/colorbox": "master",
        "phpunit/phpunit": "^8.4"
    },
    "require-dev": {
        "behat/mink-extension": "v2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "bex/behat-screenshot": "^1.2",
        "drupal/coder": "^8.2",
        "drupal/drupal-extension": "master-dev",
        "drupal/field_group": "^3.0",
        "pear/archive_tar": "^1.4.14",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
    "drupal-scaffold": {
        "locations": {
            "web-root": "web/"
        }
    },
    "installer-paths": {
        "web/core": ["type:drupal-core"],
        "web/libraries/{$name}": ["type:drupal-library"],
        "web/modules/contrib/{$name}": ["type:drupal-module"],
        "web/profiles/contrib/{$name}": ["type:drupal-profile"],
        "web/themes/contrib/{$name}": ["type:drupal-theme"],
        "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
        "web/modules/custom/{$name}": ["type:drupal-custom-module"],
        "web/themes/custom/{$name}": ["type:drupal-custom-theme"], 
        "web/libraries/colorbox": ["npm-asset/jquery-colorbox"]
    }            
    }
}
