{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "blazy"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/dinbror/blazy",
                    "reference": "1.8.2"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "fontawesome/fontawesome",
                "type": "drupal-library",
                "version": "5.9.0",
                "dist": {
                    "url": "https://use.fontawesome.com/releases/v5.9.0/fontawesome-free-5.9.0-web.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jackmoore/colorbox",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/jackmoore/colorbox/archive/1.x.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2"
                }
            }
        }
    ],
    "require": {
        "bower-asset/jquery-autoheight": "^1.0",
        "bower-asset/leaflet": "^1.7",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "dinbror/blazy": "^1.8",
        "drupal/admin_toolbar": "3.2.1",
        "drupal/adminimal_admin_toolbar": "^1.11",
        "drupal/antibot": "^2.0",
        "drupal/auto_height": "2.0.x-dev@dev",
        "drupal/backup_migrate": "^5.0",
        "drupal/betterlogin": "1.x-dev",
        "drupal/blazy": "^2.0@RC",
        "drupal/block_class": "1.x-dev",
        "drupal/block_visibility_groups": "1.x-dev",
        "drupal/bootstrap": "^3.23",
        "drupal/bootstrap_layouts": "^5.1",
        "drupal/bootstrap_library": "^1.11",
        "drupal/broken_link": "^3.1",
        "drupal/captcha": "^1.10",
        "drupal/chosen": "^2.5",
        "drupal/colorbox": "1.x-dev",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/context": "4.x-dev",
        "drupal/contribute": "^1.0@beta",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.9",
        "drupal/ctools": "^3.8",
        "drupal/devel": "^4.2",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/ds": "^3.1",
        "drupal/entity_browser": "^2.0",
        "drupal/eva": "^3.0",
        "drupal/feeds": "^3.0",
        "drupal/field_group": "^3.2",
        "drupal/fontawesome": "^2.18",
        "drupal/google_analytics": "^3.0",
        "drupal/honeypot": "^2.0",
        "drupal/httpbl": "1.x-dev",
        "drupal/libraries": "^4.0",
        "drupal/linkchecker": "1.x-dev",
        "drupal/menu_breadcrumb": "^2.0@alpha",
        "drupal/menu_token": "^9.1@alpha",
        "drupal/module_filter": "3.x-dev",
        "drupal/paragraphs": "^1.5",
        "drupal/pathauto": "^1.10",
        "drupal/recaptcha": "^3.2",
        "drupal/redirect": "^1.7",
        "drupal/restui": "^1.21",
        "drupal/search_api": "^1.23",
        "drupal/search_autocomplete": "^2.0",
        "drupal/shs": "^2.0@RC",
        "drupal/social_media": "^1.9",
        "drupal/spambot": "^1.2",
        "drupal/structure_sync": "^2.0",
        "drupal/synonyms": "2.0.x-dev",
        "drupal/taxonomy_import": "^2.0",
        "drupal/taxonomy_menu": "^3.4",
        "drupal/token": "^1.10",
        "drupal/token_filter": "^1.4",
        "drupal/upgrade_status": "^3.14",
        "drupal/views_accordion": "^2.0",
        "drupal/views_bulk_edit": "^2.0",
        "drupal/views_bulk_operations": "^3.0",
        "drupal/views_parity_row": "^1.8",
        "drupal/zircon": "9.1.x-dev@dev",
        "drush/drush": "^10.0.0",
        "harvesthq/chosen": "^1.8",
        "jackmoore/colorbox": "master",
        "kint-php/kint": "^3.3",
        "league/commonmark": "^1.6",
        "michelf/php-markdown": "^1.9",
        "npm-asset/chosen-js": "^1.8",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/.htaccess": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/chosen": ["npm-asset/chosen-js"],
            "web/libraries/autoheight": ["bower-asset/jquery-autoheight"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/libraries/colorbox": ["npm-asset/jquery-colorbox"]
        }
    }
}
